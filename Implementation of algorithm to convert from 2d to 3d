        "import pandas as pd\n",
        "\n",
        "data = pd.read_csv(\"action_dataset_images.csv\")\n",
        "df = pd.DataFrame(data)\n",
        "for ind in df.index:\n",
        "  x1 = df['x1'][ind]\n",
        "  y1 = df['y1'][ind]\n",
        "  x2 = df['x2'][ind]\n",
        "  y2 = df['y2'][ind]\n",
        "  x3 = df['x3'][ind]\n",
        "  y3 = df['y3'][ind]\n",
        "  x4 = df['x4'][ind]\n",
        "  y4 = df['y4'][ind]\n",
        "  A = 0.5*((x1*y2 + x2*y3 + x3*y4  + x4*y1 )-(x2*y1 + x3*y2 + x4*y3 + x1*y4))\n",
        "  if A == 0:\n",
        "    df.drop(df.index[ind])\n",
        "  else:\n",
        "    continue"
